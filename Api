import io.cucumber.datatable.DataTable;
import java.util.Arrays;
import java.util.List;
import java.util.Map;

public class DataTableExample {

    public static void main(String[] args) {
        // Assign values to variables
        String username1 = "user1";
        String password1 = "pass1";
        String username2 = "user2";
        String password2 = "pass2";
        String username3 = "user3";
        String password3 = "pass3";

        // Define the data as a list of lists (rows)
        List<List<String>> data = Arrays.asList(
            Arrays.asList("username", "password"),
            Arrays.asList(username1, password1),
            Arrays.asList(username2, password2),
            Arrays.asList(username3, password3)
        );

        // Create the DataTable
        DataTable dataTable = DataTable.create(data);

        // Use the DataTable (e.g., print it)
        List<Map<String, String>> asMaps = dataTable.asMaps(String.class, String.class);
        for (Map<String, String> row : asMaps) {
            System.out.println("Username: " + row.get("username") + ", Password: " + row.get("password"));
        }
    }
}