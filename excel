=SUBSTITUTE(SUBSTITUTE(A1," EST","")," PM"," PM") + TIME(10,30,0)

=DATE(YEAR(A1), MONTH(A1), DAY(A1)) + TIME(HOUR(A1)+9, MINUTE(A1)+30, SECOND(A1))

=DATEVALUE(TEXT(A1, "mm/dd/yyyy")) + TIME(HOUR(A1)+9, MINUTE(A1)+30, SECOND(A1))

=DATEVALUE(TEXT(LEFT(A1, FIND(" ", A1)-1), "mm/dd/yyyy")) + TIMEVALUE(MID(A1, FIND(" ", A1)+1, FIND(" ", A1, FIND(" ", A1)+1) - FIND(" ", A1)-1)) + TIME(9,30,0)